# Frontend .env file for Vue.js application
# Copy this file to .env and fill in your actual values
# Note: All frontend environment variables must be prefixed with VITE_

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Service URLs (Optional - for production deployment)
# These default to localhost in development mode
VITE_AUTH_SERVICE_URL=http://localhost:3001
VITE_USERS_SERVICE_URL=http://localhost:3002
VITE_PROFILES_SERVICE_URL=http://localhost:3003
VITE_BOOKINGS_SERVICE_URL=http://localhost:3004
VITE_MESSAGING_SERVICE_URL=http://localhost:3005
VITE_REVIEWS_SERVICE_URL=http://localhost:3006
VITE_NOTIFICATIONS_SERVICE_URL=http://localhost:3007
VITE_ANALYTICS_SERVICE_URL=http://localhost:3008
VITE_CALENDAR_SERVICE_URL=http://localhost:3011
VITE_MAPS_SERVICE_URL=http://localhost:3012

# Development Options (Optional)
VITE_DISABLE_HMR=false
