{
  "name": "tutorconnect",
  "version": "1.0.0",
  "description": "A marketplace for private tutors and tuition centres in Singapore",
  "main": "index.js",
  "scripts": {
    "dev": "node scripts/super-dev.js dev",
    "start": "node scripts/super-dev.js start",
    "dev:auth": "cd services/auth && npm run dev",
    "dev:users": "cd services/users && npm run dev",
    "dev:profiles": "cd services/profiles && npm run dev",
    "dev:bookings": "cd services/bookings && npm run dev",
    "dev:messaging": "cd services/messaging && npm run dev",
    "dev:reviews": "cd services/reviews && npm run dev",
    "dev:notifications": "cd services/notifications && npm run dev",
    "dev:analytics": "cd services/analytics && npm run dev",
    "dev:gamification": "cd services/gamification && npm run dev",
    "dev:earnings": "cd services/earnings && npm run dev",
    "dev:calendar": "cd services/calendar && npm run dev",
    "dev:maps": "cd services/maps && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "health": "node -e \"require('http').get('http://localhost:' + (process.env.PORT || 3000) + '/health', (res) => { process.exit(res.statusCode === 200 ? 0 : 1) })\"",
    "deploy:railway": "node scripts/deploy-railway.js",
    "deploy:setup": "npm install -g @railway/cli && railway login"
  },
  "keywords": [
    "tutor",
    "education",
    "marketplace",
    "singapore",
    "booking"
  ],
  "author": "TutorConnect Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "kill-port": "2.0.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2"
  }
}
